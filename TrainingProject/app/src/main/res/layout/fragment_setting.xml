<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/cardView.margin_top_bottom"
    android:layout_marginLeft="@dimen/cardView.margin_left_right"
    android:layout_marginRight="@dimen/cardView.margin_left_right"
    android:layout_marginTop="@dimen/cardView.margin_top_bottom"
    card_view:cardCornerRadius="@dimen/cardView.card_corner_radius">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/relativeSetting.margin_bottom">

            <ImageView
                android:id="@+id/imgAvatarSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="@dimen/leftObjects_margin_left"
                android:layout_marginStart="@dimen/leftObjects_margin_left"
                android:layout_marginTop="@dimen/imageView.user_name_setting_margin_top"
                android:background="@null"
                android:contentDescription="@string/tab_setting"
                android:src="@drawable/ico_avatar" />

            <TextView
                android:id="@+id/tvUserNameSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/textView.user_name_setting_margin_left"
                android:layout_marginStart="@dimen/textView.user_name_setting_margin_left"
                android:layout_marginTop="@dimen/textView.user_name_setting_margin_top"
                android:layout_toEndOf="@id/imgAvatarSetting"
                android:layout_toRightOf="@+id/imgAvatarSetting"
                android:text="@string/tab_setting"
                android:textColor="@color/text_user_name_setting"
                android:textSize="@dimen/textView.text.user_name_setting" />

            <ImageView
                android:id="@+id/imgBtnSettingMain"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@id/imgAvatarSetting"
                android:layout_alignEnd="@id/imgAvatarSetting"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignRight="@id/imgAvatarSetting"
                android:layout_alignTop="@id/imgAvatarSetting"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/leftObjects_margin_left"
                android:layout_marginRight="@dimen/rightObjects_margin_right"
                android:background="@null"
                android:contentDescription="@string/tab_setting"
                android:src="@drawable/ic_setting_main" />

            <View
                android:id="@+id/viewFirstLineSetting"
                android:layout_width="match_parent"
                android:layout_height="@dimen/view.height"
                android:layout_below="@id/imgAvatarSetting"
                android:layout_marginTop="@dimen/view.margin_top"
                android:background="@color/view_line_setting" />

            <ImageView
                android:id="@+id/imgTopSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/viewFirstLineSetting"
                android:layout_marginLeft="@dimen/leftObjects_margin_left"
                android:layout_marginStart="@dimen/leftObjects_margin_left"
                android:layout_marginTop="@dimen/imageView.top_setting_margin_top"
                android:background="@null"
                android:contentDescription="@string/tab_setting"
                android:src="@drawable/ic_top" />

            <TextView
                android:id="@+id/tvTopSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/viewFirstLineSetting"
                android:layout_marginLeft="@dimen/textView.top_setting_margin_left"
                android:layout_marginStart="@dimen/textView.top_setting_margin_left"
                android:layout_marginTop="@dimen/textView.top_setting_margin_top"
                android:layout_toEndOf="@id/imgTopSetting"
                android:layout_toRightOf="@+id/imgTopSetting"
                android:text="@string/textView.text.top_setting"
                android:textSize="@dimen/textView.text.user_name_setting" />


            <ImageButton
                android:id="@+id/imgBtnNextTopSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@id/imgTopSetting"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@id/imgTopSetting"
                android:layout_below="@id/viewFirstLineSetting"
                android:layout_marginEnd="@dimen/rightObjects_margin_right"
                android:layout_marginRight="@dimen/rightObjects_margin_right"
                android:background="@null"
                android:contentDescription="@string/tab_setting"
                android:src="@drawable/ic_next_top" />

            <View
                android:id="@+id/viewSecondLineSetting"
                android:layout_width="match_parent"
                android:layout_height="@dimen/view.height"
                android:layout_below="@id/imgTopSetting"
                android:layout_marginTop="@dimen/view.margin_top"
                android:background="@color/view_line_setting" />

            <ImageView
                android:id="@+id/imgAleartSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/viewSecondLineSetting"
                android:layout_marginLeft="@dimen/leftObjects_margin_left"
                android:layout_marginStart="@dimen/leftObjects_margin_left"
                android:layout_marginTop="@dimen/imageView.top_setting_margin_top"
                android:background="@null"
                android:contentDescription="@string/tab_setting"
                android:src="@drawable/ic_aleart" />

            <TextView
                android:id="@+id/tvAleartSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/viewSecondLineSetting"
                android:layout_marginLeft="@dimen/textView.aleart_setting_margin_left"
                android:layout_marginStart="@dimen/textView.aleart_setting_margin_left"
                android:layout_marginTop="@dimen/textView.aleart_setting_margin_top"
                android:layout_toEndOf="@id/imgAleartSetting"
                android:layout_toRightOf="@+id/imgAleartSetting"
                android:text="@string/textView.text.aleart_setting"
                android:textSize="@dimen/textView.text.user_name_setting" />

            <ImageButton
                android:id="@+id/imgBtnNextAlarmSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@id/imgAleartSetting"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@id/imgAleartSetting"
                android:layout_below="@id/viewSecondLineSetting"
                android:layout_marginEnd="@dimen/rightObjects_margin_right"
                android:layout_marginRight="@dimen/rightObjects_margin_right"
                android:background="@null"
                android:contentDescription="@string/tab_setting"
                android:src="@drawable/ic_next_top" />

            <View
                android:id="@+id/viewThirdLineSetting"
                android:layout_width="match_parent"
                android:layout_height="@dimen/view.height"
                android:layout_below="@id/imgAleartSetting"
                android:layout_marginTop="@dimen/view.margin_top"
                android:background="@color/view_line_setting" />

            <ImageView
                android:id="@+id/imgLogOutSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/viewThirdLineSetting"
                android:layout_marginLeft="@dimen/leftObjects_margin_left"
                android:layout_marginStart="@dimen/leftObjects_margin_left"
                android:layout_marginTop="@dimen/imageView.top_setting_margin_top"
                android:background="@null"
                android:contentDescription="@string/tab_setting"
                android:src="@drawable/ic_log_out" />

            <TextView
                android:id="@+id/tvLogOutSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/viewThirdLineSetting"
                android:layout_marginLeft="@dimen/textView.log_out_setting_margin_left"
                android:layout_marginStart="@dimen/textView.log_out_setting_margin_left"
                android:layout_marginTop="@dimen/textView.log_out_setting_margin_top"
                android:layout_toEndOf="@id/imgLogOutSetting"
                android:layout_toRightOf="@+id/imgLogOutSetting"
                android:text="@string/textView.text.log_out_setting"
                android:textSize="@dimen/textView.text.user_name_setting" />

            <ImageButton
                android:id="@+id/imgBtnNextLogOutSetting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@id/imgLogOutSetting"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@id/imgLogOutSetting"
                android:layout_below="@id/viewThirdLineSetting"
                android:layout_marginEnd="@dimen/rightObjects_margin_right"
                android:layout_marginRight="@dimen/rightObjects_margin_right"
                android:background="@null"
                android:contentDescription="@string/tab_setting"
                android:src="@drawable/ic_next_top" />

            <View
                android:id="@+id/viewFourthLineSetting"
                android:layout_width="match_parent"
                android:layout_height="@dimen/view.height"
                android:layout_below="@id/imgLogOutSetting"
                android:layout_marginTop="@dimen/view.margin_top"
                android:background="@color/view_line_setting" />
        </RelativeLayout>
    </android.support.v7.widget.CardView>
